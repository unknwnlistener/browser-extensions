{
    "manifest_version": 2,
    "name": "URL Grabber",
    "description": "Stores the current url and stores it in chrome.storage",
    "browser_action": {
        "default_popup": "index.html"
    },
    "version": "1.0.0",
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "http://*/*",
        "https://*/*"
    ]
}